                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
          <activity android:name="DataSyncOptionsActivity"
        android:screenOrientation="landscape"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
                  <activity android:name="emailSendActivity"
        android:screenOrientation="landscape"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
          <activity android:name="FinalSavedActivity"
        android:screenOrientation="landscape"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
          <activity android:name="KbdInputActivity"
        android:screenOrientation="landscape"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.KbdInputActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            
        </activity>        
        
        
        <activity android:name="dummyActivity"
        android:screenOrientation="landscape"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.dummyActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            
        </activity>  
        
    </application>
